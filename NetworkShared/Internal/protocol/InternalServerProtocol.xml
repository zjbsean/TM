      </transaction>

      <!-- 更新Portal数据 -->
      <transaction name="UpdatePortalMoniteorData">
        <param name="serverData" type="TPortalServerMoniteorData"/>
      </transaction>

      <!-- 报告服务器状态 -->
      <transaction name="ReportServerState">
        <param name="serverName" type="string"/>
